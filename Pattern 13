#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n; 
    int count = 'A';

    int i = 1;
    while (i<=n)
    {
        int j = 1;
        while (j<=n)
        {
            cout<<(char)count;    // used typecasting
            count++;
            j++;
        }
        cout<<endl;
        i++;
        
    }
    
    return 0;
}
